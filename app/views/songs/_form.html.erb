<%= simple_form_for(@song) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :key %>
    <%= f.input :lyrics %>
    <%= f.input :note %>
    <%= f.association :band %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
